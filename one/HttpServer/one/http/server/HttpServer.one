

class HttpServer
{
    void main()
    {
        String str = "hello HttpServer";
        System.co(() {
            if (str != null)
            {
                Out.println(str);
            }
            else if (str == null)
            {
                Out.println("else if");
            }
            else
            {
                Out.println("else");
            }
        });
    }
}
