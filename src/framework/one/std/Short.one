
class Short : BaseType
{
    short _value = 0;

    Short()
    {

    }

    Short(short value)
    {
        _value = value;
    }

    static Short valueOf(short value)
    {
        return new Short(value);
    }
    short value()
    {
        return _value;
    }
    virtual String toString()
    {
        return String.valueOf(_value);
    }
}