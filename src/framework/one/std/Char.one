
class Char : BaseType
{
    char _value = 0;

    Char()
    {

    }

    Char(char value)
    {
        _value = value;
    }

    static Char valueOf(char value)
    {
        return new Char(value);
    }
    char value()
    {
        return _value;
    }
    virtual String toString()
    {
        return String.valueOf(_value);
    }
}