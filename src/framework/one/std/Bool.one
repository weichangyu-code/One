
class Bool : BaseType
{
    bool _value = false;

    Bool()
    {

    }

    Bool(bool value)
    {
        _value = value;
    }

    static Bool valueOf(bool value)
    {
        return new Bool(value);
    }
    bool value()
    {
        return _value;
    }
    virtual String toString()
    {
        return _value ? "true" : "false";
    }
}
